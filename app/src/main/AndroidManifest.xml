        </activity>
        <activity android:name=".List_ActivityLayout">
            <intent-filter>
                <action android:name="mu.cs385.com.listviews_cs385.ListActivityLayout" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".List_ActivityCustom">
            <intent-filter>
                <action android:name="mu.cs385.com.listviews_cs385.ListActivityCustom" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>